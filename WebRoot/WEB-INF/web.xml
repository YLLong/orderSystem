<?xml version="1.0" encoding="UTF-8"?>
<web-app xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xmlns="http://xmlns.jcp.org/xml/ns/javaee"
	xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee http://xmlns.jcp.org/xml/ns/javaee/web-app_3_1.xsd"
	id="WebApp_ID" version="3.1">
	
	<welcome-file-list>
		<welcome-file>home</welcome-file>
	</welcome-file-list>
	
	<servlet>
		<servlet-name>foodTypeServlet</servlet-name>
		<servlet-class>com.uiyllong.servlet.FoodTypeServlet</servlet-class>
	</servlet>
	<servlet>
		<servlet-name>dinnerTableServlet</servlet-name>
		<servlet-class>com.uiyllong.servlet.DinnerTableServlet</servlet-class>
	</servlet>
	<servlet>
		<servlet-name>foodServlet</servlet-name>
		<servlet-class>com.uiyllong.servlet.FoodServlet</servlet-class>
	</servlet>
	<servlet>
		<servlet-name>homeServlet</servlet-name>
		<servlet-class>com.uiyllong.servlet.HomeServlet</servlet-class>
	</servlet>
	
	
	<servlet-mapping>
		<servlet-name>foodTypeServlet</servlet-name>
		<url-pattern>/foodtype</url-pattern>
	</servlet-mapping>
	<servlet-mapping>
		<servlet-name>dinnerTableServlet</servlet-name>
		<url-pattern>/dinnertable</url-pattern>
	</servlet-mapping>
	<servlet-mapping>
		<servlet-name>foodServlet</servlet-name>
		<url-pattern>/food</url-pattern>
	</servlet-mapping>
	<servlet-mapping>
		<servlet-name>homeServlet</servlet-name>
		<url-pattern>/home</url-pattern>
	</servlet-mapping>
	
</web-app>